<p id="notice"><%= notice %></p>

<% @exams.each do |exam| %>
    <div class="row">
      <div class="col s12 m12">
        <div class="card">
          <div class="card-content">
            <span class="card-title black-text">
              <span><%= link_to exam.code + ':' + exam.name, exam %></span>
            </span>

            <p><%= exam.description %></p>
          </div>
        </div>
      </div>
    </div>
<% end %>

<br>